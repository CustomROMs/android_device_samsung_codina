From ede16c2cb3cd38357016a323b87ebebf78150df5 Mon Sep 17 00:00:00 2001
From: Shilin Victor <chrono.monochrome@gmail.com>
Date: Mon, 17 Sep 2018 05:08:51 +0300
Subject: [PATCH 25/31] logd: don't set group to root

Change-Id: Ic7feca4dea2c081b0d5b4c0bc0aec7cc7fe7557f
---
 logd/logd.rc | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/logd/logd.rc b/logd/logd.rc
index cbd45aaa2..4d7217884 100644
--- a/logd/logd.rc
+++ b/logd/logd.rc
@@ -5,14 +5,14 @@ service logd /system/bin/logd
     file /proc/kmsg r
     file /dev/kmsg w
     user root
-    group root logd system package_info readproc
+    group logd system package_info readproc
     writepid /dev/cpuset/system-background/tasks
 
 service logd-reinit /system/bin/logd --reinit
     oneshot
     disabled
     user root
-    group root
+    group logd
     writepid /dev/cpuset/system-background/tasks
 
 on fs
-- 
2.11.0

